<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.urassh.gacha_app.controller.ResultController">
    <Label fx:id="resultLabel" text="Press the button to roll!" style="-fx-font-size: 16;" />
    <Button text="Roll Gacha" onAction="#handleRoll" />
    <Button text="Back to Home" onAction="#handleBackToHome" />
</VBox>
